OpenArabicPE_cb =
  
  ## (column beginning) marks the beginning of a new column of a text on a multi-column page. [3.11.3. Milestone
  ## Elements]
  element cb {
    empty,
    OpenArabicPE_att.global.attributes,
    OpenArabicPE_att.typed.attributes,
    
    ## (edition) supplies a sigil or other arbitrary identifier for the source edition in which the associated feature (for example, a page, column, or line break) occurs at this point in the text.
    ## Suggested values include: 1] print; 2] shamela
    [ a:defaultValue = "print" ]
    attribute ed {
      list {
        (
         ## Column beginning, i.e. break, according to a printed edition
         "print"
         | 
           ## Column beginning, i.e. break, as found in the transcription from Maktabat al-Shamela
           "shamela"
         | xsd:token { pattern = "[^\p{C}\p{Z}]+" })+
      }
    }?,
    [
      a:defaultValue = "#edition_1"
      a:documentation [
        "(edition reference) Points to the "
        ns1:code [ "@xml:id" ]
        " of the corresponding print edition\x{a}" ~
        "Suggested values include: 1] #edition_1"
      ]
    ]
    attribute edRef {
      list {
        ([
           a:documentation [
             "Points to an exemplary "
             ns1:code [ "@xml:id" ]
             " of a "
             ns1:code [ "<biblStruct>" ]
             " describing the corresponding print edition."
           ]
         ]
         "#edition_1"
         | xsd:anyURI)+
      }
    }?,
    empty
