     OpenArabicPE_imprint,
     (OpenArabicPE_imprint | OpenArabicPE_biblScope)*),
    OpenArabicPE_att.global.attributes,
    
    ## This attribute specifies the publication frequency
    ## Suggested values include: 1] daily; 2] weekly; 3] biweekly; 4] fortnightly; 5] monthly; 6] annually
    [ a:defaultValue = "monthly" ]
    attribute oape:frequency {
      
      ## Published daily, with the possible exception of weekends and holidays
      "daily"
      | 
        ## Published once a week
        "weekly"
      | 
        ## Published twice a week
        "biweekly"
      | 
        ## Published once every two weeks
        "fortnightly"
      | 
        ## Pubslihed once a month
        "monthly"
      | 
        ## Published once a year
        "annually"
      | xsd:string
    }?,
    
    ## This attribute specifies the weekday of publication. Values use common English abbreviations.
    ## Suggested values include: 1] mon; 2] tue; 3] wed; 4] thu; 5] fri; 6] sat; 7] sun
    attribute oape:weekday {
      
