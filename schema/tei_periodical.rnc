             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes @target' and @cRef' may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    OpenArabicPE_att.cReferencing.attributes,
    OpenArabicPE_att.global.attributes,
    OpenArabicPE_att.internetMedia.attributes,
